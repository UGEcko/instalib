{
  "name": "instalib",
  "displayName": "instaLib",
  "description": "Send code snippets to a github file and/or library",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "instalib.helloWorld",
        "title": "-- Export to github library --"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "instalib.helloWorld",
          "group": "navigation",
          "when": "editorHasSelection"
        }
      ]
    },
    "configuration": { 
      "type": "object",
      "properties": {
          "instalib.accessToken": {
              "type": "string",
              "default": ""
          },
          "instalib.owner": {
              "type": "string",
              "default": ""
          },
          "instalib.repo": {
              "type": "string",
              "default": ""
          },
          "instalib.filePath": {
              "type": "string",
              "default": ""
          }
      }
  }
  },
  "enabledApiProposals": [
    "contribEditorContentMenu"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.88.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.8"
  }
}
